<?xml version="1.0" encoding="UTF-8"?>
<confiuration status="INFO">
    <Appenders>
        <console name="Console" target="SYSTEM_OUT">
            <PatternLayout pattern="%d{HH:mm:ss} [%t] %-5level %logger{36} - %msg%n" />
        </console>
    </Appenders>
    <Loggers>
        <root level="debug">
            <AppenderRef ref="Console" />
        </root>
    </Loggers>
</confiuration>